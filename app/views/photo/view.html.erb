<h1 style="text-align: center"><%= photo.caption %></h1>
<div class="wrapper">
	<%= token_tag %>
    <div class="photo">
		<div>
		    <a href = "/photo/<%= photo.id %>">
		        <img class="photo-image" src="<%= photo.link %>">
		    </a>
		</div>
		<p class="photo-caption"><%= photo.caption %></p>
		<form class="deletes" method="POST" action="/photo/delete" enctype="multipart/form-data">
		  <%= token_tag %> 
		  <input type="hidden" name="id" value="<%= photo.id %>">
		  <imput type="hidden" name="link" value="<%= photo.link%>">
		  <input type="submit" value="Delete">
		</form>
	</div>
</div>
<a href ='/'><h2 style="text-align: center">Back to feed</h2></a>